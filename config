#!/usr/bin/env bash
export KIBANA_IMAGE=${KIBANA_IMAGE:="kibana"}
export KIBANA_IMAGE_VERSION=${KIBANA_IMAGE_VERSION:="4.4.1"}
export KIBANA_ROOT=${KIBANA_ROOT:="/var/lib/dokku/services/kibana"}

export PLUGIN_COMMAND_PREFIX="kibana"
export PLUGIN_DATA_ROOT=$KIBANA_ROOT
export PLUGIN_FRONTEND_PORTS=(5601)
export PLUGIN_DEFAULT_ALIAS="KIBANA"
export PLUGIN_ALT_ALIAS="DOKKU_KIBANA"
export PLUGIN_IMAGE=$KIBANA_IMAGE
export PLUGIN_IMAGE_VERSION=$KIBANA_IMAGE_VERSION
export PLUGIN_SCHEME="http"
export PLUGIN_SERVICE="Kibana"
